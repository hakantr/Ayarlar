layout {
    pane split_direction="vertical" {
        pane edit="src/main.rs" 
        focus true  
    }
    pane split_direction="vertical" size=15 {
        pane command="cargo" {
            args "run"  
        }
    }
    pane size=1 borderless=true {
        plugin location="zellij:tab-bar"
    }
}
